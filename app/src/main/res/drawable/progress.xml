<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:id="@android:id/progress"
        >
        <clip>
            <shape>
                <corners
                    android:radius="5dip" />
                <gradient
                    android:startColor="@color/colorProgressLine"
